<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel"
                version="11.1.1.55.36" id="principalPageDef"
                Package="oracle.pageDefs">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="root" RangeSize="25" DataControl="HRFacadeLocal"
              id="HRFacadeLocalIterator" ChangeEventPolicy="ppr"/>
    <accessorIterator MasterBinding="HRFacadeLocalIterator"
                      Binds="articuloFindAll" RangeSize="25"
                      DataControl="HRFacadeLocal" BeanClass="oracle.Articulo"
                      id="articuloFindAllIterator" ChangeEventPolicy="ppr"/>
    <accessorIterator MasterBinding="articuloFindAllIterator" Binds="vendedor"
                      RangeSize="25" DataControl="HRFacadeLocal"
                      BeanClass="oracle.Vendedor" id="vendedorIterator"
                      ChangeEventPolicy="ppr"/>
  </executables>
  <bindings>
    <attributeValues IterBinding="articuloFindAllIterator" id="baños">
      <AttrNames>
        <Item Value="baños"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="vendedorIterator" id="usuario">
      <AttrNames>
        <Item Value="usuario"/>
      </AttrNames>
    </attributeValues>
    <tree IterBinding="articuloFindAllIterator" id="articuloFindAll">
      <nodeDefinition DefName="oracle.Articulo">
        <AttrNames>
          <Item Value="baños"/>
          <Item Value="ciudad"/>
          <Item Value="codigo"/>
          <Item Value="codiInterno"/>
          <Item Value="descripcion"/>
          <Item Value="destacada"/>
          <Item Value="direccion"/>
          <Item Value="estructura"/>
          <Item Value="fecha"/>
          <Item Value="monto"/>
          <Item Value="piezas"/>
          <Item Value="pisos"/>
          <Item Value="superficie"/>
          <Item Value="superficieconstruida"/>
          <Item Value="tipo"/>
          <Item Value="titulo"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <attributeValues IterBinding="articuloFindAllIterator" id="titulo">
      <AttrNames>
        <Item Value="titulo"/>
      </AttrNames>
    </attributeValues>
    <action IterBinding="articuloFindAllIterator" id="First"
            RequiresUpdateModel="true" Action="first"/>
    <action IterBinding="articuloFindAllIterator" id="Previous"
            RequiresUpdateModel="true" Action="previous"/>
    <action IterBinding="articuloFindAllIterator" id="Next"
            RequiresUpdateModel="true" Action="next"/>
    <action IterBinding="articuloFindAllIterator" id="Last"
            RequiresUpdateModel="true" Action="last"/>
  </bindings>
</pageDefinition>
